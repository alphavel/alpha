{
    "name": "alphavel/alpha",
    "description": "Alphavel Framework CLI - Intelligent code generation and scaffolding toolkit",
    "keywords": [
        "alphavel",
        "cli",
        "console",
        "code-generation",
        "scaffolding",
        "framework",
        "artisan"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Arthur",
            "email": "arthur@alphavel.dev"
        }
    ],
    "type": "library",
    "require": {
        "php": ">=8.4",
        "alphavel/alphavel": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "suggest": {
        "alphavel/database": "Required for schema-aware code generation and model scaffolding"
    },
    "autoload": {
        "psr-4": {
            "Alphavel\\Alpha\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Alphavel\\Alpha\\Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/alpha"
    ],
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
